
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br/>
<h4>Yeni Deneyim Ekleme Sayfası</h4>

<form method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlık yazınız." />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık yazınız." />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarih yazınız." />
    <br />
    <textarea name="Aciklama" placeholder="Açıklama Giriniz " class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

